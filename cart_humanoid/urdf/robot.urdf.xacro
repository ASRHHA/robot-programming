<?xml version="1.0"?>
<robot name="cart_humanoid" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find kobuki_description)/urdf/kobuki.urdf.xacro"/>
  <xacro:include filename="$(find turtlebot_description)/urdf/stacks/hexagons.urdf.xacro" />
  <xacro:include filename="$(find turtlebot_description)/urdf/sensors/kinect.urdf.xacro" />
  <!-- <xacro:include filename="$(find dynamixel_7dof_arm)/urdf/dynamixel_7dof_arm.urdf.xacro" /> -->
  <xacro:include filename="$(find cart_humanoid)/urdf/JAXON_RED.urdf.xacro" />
  
  <!-- <kobuki/> -->
  <JAXON_RED/>

  <!-- <joint name="joint0" type="fixed"> -->
  <!--   <parent link="plate_top_link"/> -->
  <!--   <child link="BODY"/> -->
  <!--   <origin xyz="0 0 1" rpy="0 0 0" /> -->
  <!-- </joint> -->

  <link name="world"/>
  <joint name="slide_x" type="prismatic">
    <parent link="world"/>
    <child link="slide_y_link"/>
    <limit effort="1000.0" lower="-1" upper="1" velocity="0.5"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 1" rpy="0 0 0" />
  </joint>

  <link name="slide_y_link">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>
  
  <joint name="slide_y" type="prismatic">
    <parent link="slide_y_link"/>
    <child link="BODY"/>
    <limit effort="1000.0" lower="-1" upper="1" velocity="0.5"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>
  
  <!-- <stack_hexagons parent="base_link"/> -->
  <!-- <sensor_kinect  parent="base_link"/> -->

